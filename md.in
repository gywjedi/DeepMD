&CONTROL
  calculation      = 'md'
  pseudo_dir       = './PseudoPotential'
  prefix           = 'PAN'
  outdir           = './outdir/'
  tstress          = .true.         ! virial/stress for DeepMD
  tprnfor          = .true.         ! forces for DeepMD
  disk_io          = 'low'
  restart_mode     = 'from_scratch'
  nstep            = 20000          ! ~9.6 ps if dt=20 a.u. (0.48 fs)
  dt               = 20.0           ! a.u. of time (~0.48 fs)
  verbosity        = 'high'
  forc_conv_thr    = 1.0D-4
  etot_conv_thr    = 1.0D-5
/
&SYSTEM
  ibrav            = 0
  nat              = 78
  ntyp             = 3
  ecutwfc          = 47.0
  ecutrho          = 326.0
  vdw_corr         = 'grimme-d3'
  occupations      = 'smearing'     ! ok for finite-T MD
  smearing         = 'gaussian'
  degauss          = 0.002
  nosym            = .true.         ! avoid symmetry operations during MD
  noinv            = .true.
/
&ELECTRONS
  electron_maxstep = 200
  conv_thr         = 1.0D-7
  mixing_beta      = 0.7
  scf_must_converge = .true.
/
&IONS
  ion_dynamics     = 'verlet'       ! Born–Oppenheimer MD integrator
  ion_temperature  = 'andersen'     ! simple, stable thermostat
  tempw            = 600.0          ! target temperature (K) – adjust as needed
  pot_extrapolation= 'second_order' ! smoother BO extrapolation
  wfc_extrapolation= 'second_order'
/
&CELL
  cell_dynamics    = 'none'         ! NVT (fixed cell). For NPT, see note below.
/

ATOMIC_SPECIES
C 12.011  C.pbe-n-kjpaw_psl.1.0.0.UPF
